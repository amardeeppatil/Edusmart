<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.edusmart.dao.City" table="CityTB">
		<id name="CityId" type="int" column="CityId" access="field">
         	<generator class="increment"/>
      	</id>
		<property name="CityName" type="java.lang.String"></property>
		<property name="StateId" type="int"></property>
		<property name="CountryID" type="int"></property>
		<property name="User_ID" type="int"></property>
		<property name="Status" type="int"></property>
	</class>

</hibernate-mapping>